<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Zdom</servlet-name>
		<servlet-class>com.ddetyuk.ZdomServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Zdom</servlet-name>
		<url-pattern>/zdom</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Adduser</servlet-name>
		<servlet-class>com.ddetyuk.AdduserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Adduser</servlet-name>
		<url-pattern>/adduser</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Test</servlet-name>
		<servlet-class>com.ddetyuk.TestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Persistent</servlet-name>
		<servlet-class>com.ddetyuk.PersistentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Persistent</servlet-name>
		<url-pattern>/persistent</url-pattern>
	</servlet-mapping>
	
		
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<context-param>
		<param-name>javax.servlet.jsp.jstl.fmt.timeZone</param-name>
		<param-value>GMT+2:00</param-value>
	</context-param> 
</web-app>
